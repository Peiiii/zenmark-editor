{
  "name": "zenmark-editor",
  "version": "0.0.2",
  "description": "A Markdown editor built on Tiptap/ProseMirror with a simple React API.",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "files": [
    "dist"
  ],
  "types": "./dist/es/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && vite --config lib.vite.config.ts build",
    "dev": "vite --config lib.vite.config.ts",
    "watch": "vite --config lib.vite.config.ts build --watch",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "prepublishOnly": "pnpm run build",
    "release": "npm publish --access public"
  },
