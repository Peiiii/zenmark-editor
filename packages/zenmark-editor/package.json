{
  "name": "zenmark-editor",
  "version": "0.1.21",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "typings": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite --host --port 3000",
    "build": "tsc && vite --config lib.vite.config.ts build",
    "build:lib": "tsc && vite --config lib.vite.config.ts build",
    "watch": "bash -lc 'tsc -w & vite --config lib.vite.config.ts build --watch'",
    "preview": "vite preview --host --port 3000",
    "deploy": "yarn build && yarn oss:upload",
    "upload:lib": "ossutil64 cp -r -u build/lib/tiptap-editor oss://write-dimstack-com/zenmark-editor/lib -c ./.ossutilconfig.local",
    "deploy:lib": "yarn build:lib && ossutil64 cp -r -u build/lib/tiptap-editor oss://write-dimstack-com/zenmark-editor/lib -c ./.ossutilconfig.local",
    "oss:upload": "ossutil64 cp -r -u build/tiptap-editor oss://write-dimstack-com/zenmark-editor -c ./.ossutilconfig.local && ossutil64 cp -r -u build/wc/tiptap-editor oss://write-dimstack-com/zenmark-editor/wc  -c ./.ossutilconfig.local",
    "git:update": "git add . && git commit -m 'update' && git push",
    "update": "yarn git:update  &&  yarn deploy:lib && yarn deploy",
    "release": "bumpp && pnpm build && pnpm publish"
  },
  "peerDependencies": {
    "@benrbray/prosemirror-math": "^0.2.2",
