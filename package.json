{
  "name": "tiptap-editor",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host --port 3000",
    "build": "yarn build:app && yarn build:wc",
    "build:app": "tsc && vite build",
    "build:wc": "tsc && vite --config wc.vite.config.ts build",
    "preview": "vite preview --host --port 3000",
    "oss:upload": "ossutil64 cp -r -u build/tiptap-editor oss://apps-eiooie-com/tiptap-editor && ossutil64 cp -r -u build/wc/tiptap-editor oss://apps-eiooie-com/wc/tiptap-editor",
    "update": "pkx gitops update && yarn oss:upload"
  },
